<div class="row">
    <div class="col-lg-12 col-md-12 col-sm-12">
        <div class="well well-lg">

            <div id="contact-message-box">

                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <hgroup class="title">
                            <h2 class="page-headers">Info.</h2>

                        </hgroup>
                    </div>
                </div>

                @*  START SCREENING FORM  *@

                <div class="row">
                    <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                        <form action="~/Scripts/process.php" method="post" class="form-horizontal" id="contactForm">
                            <fieldset>

                                <div class="row">
                                    @*  FULL NAME  *@
                                    <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12" id="contact-name-email-col">
                                        <div class="form-group">
                                            <header>
                                                <h4 class="contact-page-headers">Full Name:</h4>
                                            </header>

                                            <input type="text" class="form-control" id="fullName" placeholder="Full Name (Required)" autocomplete="off" style="cursor: auto; background-image: url(data:image/png;base64,); background-attachment: scroll; background-position: 100% 50%; background-repeat: no-repeat;" required>
                                        
                                            <header id="contact-email-header">
                                                <h4 class="contact-page-headers">Email:</h4>
                                            </header>

                                            <input type="email" class="form-control" id="email" placeholder="Email (Required)" autocomplete="off" style="cursor: auto; background-image: url(data:image/png;base64,); background-attachment: scroll; background-position: 100% 50%; background-repeat: no-repeat;" required>

                                            <header id="contact-phone-header">
                                                <h4 class="contact-page-headers">Phone:</h4>
                                            </header>

                                            <input type="number" class="form-control" id="phone" placeholder="Phone (Required)" autocomplete="off" style="cursor: auto; background-image: url(data:image/png;base64,); background-attachment: scroll; background-position: 100% 50%; background-repeat: no-repeat;" required>

                                        </div>
                                    </div class="col-lg-6 col-md-6 col-sm-12 col-xs-12">

                                    <div class="col-lg-6 col-md-6 col-sm-12 col-xs-12" id="generalComments-col">



                                        @*  GENERAL COMMENTS  *@
                                        <header>
                                            <h4 id="contact-page-headers">Message:</h4>
                                        </header>


                                        <textarea class="form-control" rows="5" id="message" placeholder="Use this space to introduce and tell me a little bit about yourself.  In order to facilitate my reply please include all of your pertinant contact information (Phone/Email/Etc) as well as TWO references for providers you have seen within the past year. Make sure to list each reference's WORKING phone/email."></textarea>
                                        <span class="help-block"></span>
                                    </div>
                                </div>

                                <br />

                                @*  SUBMIT AND CANCEL BUTTONS  *@
                                <div class="row">
                                    <div class="form-group">
                                        <div class="col-lg-12" id="contact-buttons" align="center">

                                            <button id="contact-submit" type="submit" class="btn btn-default">Submit</button>
                                            <button id="contact-cancel" type="reset" class="btn btn-default">Cancel</button>

                                        </div>
                                    </div>
                                </div>
                            </fieldset>
                        </form>
                    </div>
                </div>
            </div>


        </div>
    </div>
</div>


